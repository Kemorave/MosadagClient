<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Title>Mosadag API client</Title>
    <Authors>Kemorave</Authors>
    <Company>Ensight-tech</Company>
    <Description>An api client for Mosadag plugin</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://ensight-tech.de/</PackageProjectUrl>
    <PackageIcon>mosadagClient.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/Kemorave/MosadagClient/tree/master/MosadagClient</RepositoryUrl>
    <PackageTags>API, Ensight-tech</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion></AssemblyVersion>
    <Version>2.0.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Supabase" Version="1.1.1" />
  </ItemGroup>

  <ItemGroup>
    <None Update="mosadagClient.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
